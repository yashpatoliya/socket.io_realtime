  "bugs": {
    "url": "https://github.com/yashpatoliya/socket.io_realtime/issues"
  },
  "homepage": "https://github.com/yashpatoliya/socket.io_realtime#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "nodemon": "^3.0.3",
    "socket.io": "^4.7.4"
  }
}

