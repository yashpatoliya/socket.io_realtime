{
  "name": "socket.io_realtime",
  "version": "1.0.0",
  "description": "Socket Mern Stack",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "server": "nodemon server/index.js",
    "frontend": "npm start --prefix client",
